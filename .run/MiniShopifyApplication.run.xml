<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Run Mini Shopify" type="Application" factoryName="Application">
    <envs>
      <env name="DB_HOST" value="localhost" />
      <env name="DB_PORT" value="5432" />
      <env name="DB_NAME" value="shopify" />
      <env name="DB_USER" value="postgres" />
      <env name="DB_PASS" value="postgres" />
      <env name="FLYWAY_DEV" value="true" />
      <env name="JWTSecret" value="SomeRandomSecret" />
    </envs>
    <option name="MAIN_CLASS_NAME" value="com.shopify.minishopify.MiniShopifyApplication" />
    <module name="minishopify" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.shopify.minishopify.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>